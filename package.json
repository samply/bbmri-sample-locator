{
  "name": "bbmri-sample-locator",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "release": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./**/*.{js,svelte,html,ts}",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "prepare": "svelte-kit sync && husky install",
    "security:check": "npm audit",
    "version": "npm version -m \"build: release version %s\""
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.4.0",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/eslint": "^9.6.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.42.2",
    "svelte-check": "^4.3.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@samply/lens": "^0.6.2",
    "uuid": "^13.0.0"
  }
}
